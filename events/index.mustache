---
# WARNING
# This content is automatically generated
# Do not edit this file by hand
#

title: {{ title }}
event: {{ title }}
event_url: {{ website }}

location: {{ location }}

address:
  street: {{ address.street }}
  city: {{ address.city }}
  region: {{ address.region }}
  postcode: {{ address.postcode }}
  country: {{ address.country }}

summary: {{#summary}}|
         {{ summary }}{{/summary}}

abstract: {{#abstract}}|
          {{ abstract }}{{/abstract}}

# Talk start and end times.
#   End time can optionally be hidden by prefixing the line with `#`.
date: "{{ date }}"
date_end: "{{ date_end }}"
all_day: true

# Schedule page publish date (NOT event date).
publishDate: "{{ publishDate }}"

authors: []
tags: []

# Is this a featured talk? (true/false)
featured: false

image:
  caption: "{{#image_caption}}{{ image_caption }}{{/image_caption}}"
  focal_point: center

links:
{{#website}}
  - icon: globe
    icon_pack: fas
    name: Website
    url: {{ website }}
{{/website}}

{{#twitter_handle}}
  - icon: twitter
    icon_pack: fab
    name: "@{{ twitter_handle }}"
    url: https://twitter.com/{{ twitter_handle }}
{{/twitter_handle}}

{{#mattermost_channel}}
  - icon: mattermost
    icon_pack: custom
    name: Mattermost
    url: https://mattermost.brainhack.org/brainhack/channels/{{ mattermost_channel }}
{{/mattermost_channel}}

{{#facebook}}
  - icon: facebook
    icon_pack: fab
    name: Facebook
    url: https://www.facebook.com/{{ facebook }}
{{/facebook}}

{{#instagram}}
  - icon: instagram
    icon_pack: fab
    name: Instagram
    url: https://www.instagram.com/{{ instagram }}
{{/instagram}}

{{#github_username}}
  - icon: github
    icon_pack: fab
    name: Github
    url: https://www.github.com/{{ github_username }}
{{/github_username}}

{{!
# Projects (optional).
#   Associate this post with one or more of your projects.
#   Simply enter your project's folder or file name without extension.
#   E.g. `projects = ["internal-project"]` references `content/project/deep-learning/index.md`.
#   Otherwise, set `projects = []`.
projects:
# - project_84

# Enable math on this page?
math: true
}}
---
